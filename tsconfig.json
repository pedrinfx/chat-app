{
    "compilerOptions": {
        "target": "es2016",
        "module": "commonjs",
        "esModuleInterop": true,
        "forceConsistentCasingInFileNames": true,
        "strict": true,
        "skipLibCheck": true,
        "baseUrl": ".",
        "paths": {
            "@services/*": ["./src/services/*"],
            "@routers/*": ["./src/routers/*"],
            "@controllers/*": ["./src/controllers/*"],
            "@prisma": ["./src/prisma/prisma"],
            "@app": ["./src/app"],
            "@middlewares/*": ["./src/middlewares/*"]
        },
        "outDir": "./dist",
        "typeRoots": ["./src/@types", "node_modules/@types"]
    }
}
